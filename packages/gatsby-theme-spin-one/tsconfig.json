{
  "compilerOptions": {
    "target": "ESNext",
    // https://www.gatsbyjs.com/docs/how-to/custom-configuration/es-modules/#current-limitations
    "module": "CommonJS",
    "moduleResolution": "Node",
    "jsx": "react",
    // "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "./"
  },
  // "include": ["gatsby-config.ts", "gatsby-node.ts", "gatsby-ssr.tsx", "gatsby-browser.tsx", "src/**/*.ts"],
  "include": ["./*.ts", "gatsby-browser.tsx", "src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
