{
  "compilerOptions": {
    "target": "ESNext",
    // https://www.gatsbyjs.com/docs/how-to/custom-configuration/es-modules/#current-limitations
    "module": "CommonJS",
    "moduleResolution": "Node",
    "jsx": "react",
    // "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  },
  "include": ["**/*", "data/**/*.json"]
}
