{
  "name": "@spin-dd/gatsby-theme-spin-one",
  "version": "0.0.1",
  "author": "K.K. SPIN D&D <dev@spin-dd.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/spin-dd/spin-one/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spin-dd/spin-one.git",
    "directory": "packages/gatsby-theme-spin-one"
  },
  "main": "index.js",
  "types": "index.ts",
  "bin": {
    "contentful-setup": "./dist/scripts/contentful-setup.js",
    "contentful-sync": "./dist/scripts/contentful-sync.js"
  },
  "files": [
    "libs",
    "dist",
    "src",
    "*.js"
  ],
  "scripts": {
    "lint": "run-s -c lint:*",
    "lint:es": "ESLINT_USE_FLAT_CONFIG=true eslint **/*.ts **/*.tsx",
    "lint:ts": "tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "lint-fix": "run-s lint-fix:*",
    "lint-fix:es": "npm run lint:es -- --fix",
    "lint-fix:prettier": "npm run lint:prettier -- --write",
    "build": "run-s -c build:*",
    "build:clean": "rm -rf dist",
    "build:theme-gatsby": "tsc --project tsconfig.json",
    "build:scripts": "tsc --project scripts/tsconfig.json",
    "postbuild": "chmod +x dist/scripts/contentful-setup.js dist/scripts/contentful-sync.js"
  },
  "dependencies": {
    "bootstrap": "5.3.3",
    "contentful-import": "9.4.70",
    "contentful-management": "11.44.0",
    "escape-html": "1.0.3",
    "gatsby-plugin-google-tagmanager": "5.14.0",
    "gatsby-plugin-image": "3.14.0",
    "gatsby-plugin-sass": "6.14.0",
    "gatsby-plugin-sitemap": "6.14.0",
    "gatsby-plugin-typescript": "5.14.0",
    "gatsby-source-contentful": "8.14.0",
    "gatsby-transformer-remark": "6.14.0",
    "html-to-react": "1.7.0",
    "react-bootstrap": "2.10.5"
  },
  "devDependencies": {
    "@types/escape-html": "1.0.4",
    "@types/mime": "3.0.4",
    "contentful-cli": "3.5.4",
    "dotenv": "16.4.5",
    "gatsby-plugin-schema-snapshot": "4.14.0",
    "graphql-config": "5.1.3",
    "mime": "3.0.0",
    "node-html-parser": "6.1.13"
  },
  "peerDependencies": {
    "gatsby": "5.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
