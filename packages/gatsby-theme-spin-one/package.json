{
  "name": "@spin-dd/gatsby-theme-spin-one",
  "version": "0.0.1",
  "author": "K.K. SPIN D&D <dev@spin-dd.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/spin-dd/spin-one/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spin-dd/spin-one.git",
    "directory": "packages/gatsby-theme-spin-one"
  },
  "main": "index.js",
  "bin": {
    "contentful-setup": "./dist/scripts/contentful-setup.js",
    "contentful-media-sync": "./dist/scripts/contentful-media-sync.js"
  },
  "files": [
    "dist",
    "src",
    "gatsby-config.js",
    "gatsby-node.js",
    "gatsby-browser.js",
    "gatsby-ssr.js",
    "README.md"
  ],
  "scripts": {
    "lint": "run-s -c lint:*",
    "lint:es": "eslint .",
    "lint:ts": "tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "lint-fix": "run-s lint-fix:*",
    "lint-fix:es": "npm run lint:es -- --fix",
    "lint-fix:prettier": "npm run lint:prettier -- --write",
    "build": "run-s -c build:*",
    "build:clean": "rm -rf dist",
    "build:theme": "tsc --project tsconfig.json",
    "build:scripts": "tsc --project tsconfig.scripts.json",
    "postbuild": "chmod +x dist/scripts/contentful-setup.js dist/scripts/contentful-media-sync.js"
  },
  "dependencies": {
    "bootstrap": "5.3.3",
    "contentful": "11.2.0",
    "contentful-import": "9.4.64",
    "contentful-management": "11.35.1",
    "gatsby-plugin-google-tagmanager": "5.13.1",
    "gatsby-plugin-image": "3.13.1",
    "gatsby-plugin-sass": "6.13.1",
    "gatsby-plugin-sitemap": "6.13.1",
    "gatsby-plugin-typescript": "5.13.1",
    "gatsby-source-contentful": "8.13.2",
    "gatsby-transformer-remark": "6.13.1",
    "html-to-react": "1.7.0",
    "react-bootstrap": "2.10.5"
  },
  "devDependencies": {
    "contentful-cli": "3.4.5",
    "dotenv": "16.4.5",
    "graphql-config": "5.1.3",
    "mime": "4.0.4",
    "node-html-parser": "6.1.13"
  },
  "peerDependencies": {
    "gatsby": "5.13.7",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
