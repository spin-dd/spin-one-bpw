  "license": "UNLICENSED",
  "scripts": {
    "init": "run-s -c init:*",
    "init:contentful-setup": "npx contentful-setup",
    "init:contentful-sync": "npx contentful-sync demo-data",
    "demo": "run-s build-theme clean develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "postbuild": "cp src/gatsby-types.d.ts ../gatsby-theme-spin-one",
    "build-theme": "npm run build -w  ../gatsby-theme-spin-one",
    "update-schema": "GATSBY_UPDATE_SCHEMA_SNAPSHOT=true gatsby build",
    "postupdate-schema": "cp .cache/schema.gql ../gatsby-theme-spin-one",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@spin-dd/gatsby-theme-spin-one": "*",
    "gatsby": "5.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "gatsby-plugin-schema-snapshot": "4.14.0",
    "@contentful/rich-text-types": "17.0.0",
    "@types/node": "20.17.6",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1"
  }
}
